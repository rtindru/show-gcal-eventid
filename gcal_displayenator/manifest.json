{
  "name": "GCal Details Displayenator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Displays Google Calendar Event IDs & Calendar IDs",
  "homepage_url": "http://www.github.com/rtindru",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/display_eids.js"]
    }
  ],
  "default_locale": "en",
  "background": {
    "scripts": ["src/bg/background.js"]
  },
  "browser_action": {
    "default_title": "Show GCal EventIDs"
  },
  "permissions": ["activeTab"]
}